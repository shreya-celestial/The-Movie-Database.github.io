{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prefer-const": "warn",
        "no-unused-vars": "off",
        "no-undef": "off",
        "eqeqeq": "warn",
        "semi": "warn"
    }
}
